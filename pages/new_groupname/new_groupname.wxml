<!--pages/new_groupname/new_groupname.wxml-->
<view  class="topbar" style="background-color:gray;">
<text>原来的群称：</text>
<text style="margin: auto;">{{group_name}}</text>
</view>
<view class="container">
<form bindsubmit="formSubmit" bindreset="formReset">
  <view class="section">
    <view class="section-title">设置新的群称：</view>
    <view class="section-input">
        <input name="name" type="text" placeholder="在此输入" />
    </view>
  </view>
  <view class="btn-area">
    <button formType="submit" type="primary">提交群称</button>
    <button formType="reset" type="default">取消更改</button>
  </view>
</form>
</view>
